{
    "name": "jovertpalonpon/laradash",
    "description": "Dashboard builder on top of Laravel framework",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "package"
    ],
    "authors": [
        {
            "name": "Jovert Palonpon",
            "email": "palonponjovertlota@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "inertiajs/inertia-laravel": "^0.2.5",
        "laravel/framework": "^6.0|^7.0",
        "tightenco/ziggy": "^0.9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "JovertPalonpon\\Laradash\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "format": "php-cs-fixer fix ."
    },
    "extra": {
        "laravel": {
            "providers": [
                "JovertPalonpon\\Laradash\\LaradashServiceProvider"
            ]
        }
    }
}
